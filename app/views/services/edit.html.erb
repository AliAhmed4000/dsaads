<% if params[:type] == "overview" || params[:type].blank?%>
	<%= render 'ower_view_fields', service: @service %>
<% elsif params[:type] == "pricing" %>
	<%= render 'package_fields', service: @service %>
<% elsif params[:type] == "description" %>
	<%= render 'description_fields', service: @service %>
<% elsif params[:type] == "requirement" %>
	<%= render 'requirements_fields', service: @service %>
<% elsif params[:type] == "gallery" %>
	<%= render 'gallery', service: @service %>
<% elsif params[:type] == "publish" %>
	<%= render 'publish', service: @service %>
<% end %>

  