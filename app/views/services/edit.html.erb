<% if params[:type] == "pricing" %>
	<%= render 'package_fields', service: @service %>
<% elsif params[:type] == "description" %>
	<%= render 'description_fields', service: @service %>
<% elsif params[:type] == "requirement" %>
	<%= render 'requirements_fields', service: @service %>
