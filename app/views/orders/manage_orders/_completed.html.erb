<div class="tab-pane" id="completed">
<div class="panel-body">
    <div class="table-responsive">
      <table class="table responsive table-hover table-condensed mb25" data-sortable data-sortable-initialized="true">
        <thead>
          <tr>
            <th>
              <% if current_user.seller? %>
              BUYER
              <% else %>
              SELLER
              <% end %>
            </th>
            <th>GIG</th>
            <th>ORDER DATE</th>
            <th>DUE DATE</th>
            <th>TOTAL</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% @order_completed.each do |item| %>
            <tr>
              <td class="align-middle">
                <% if current_user.sellers? %>
                  <%= link_to item.order.user.full_name,profile_path(item.order.user) %>
                <% else %>
                  <%= link_to item.package.service.seller.full_name,profile_path(item.package.service.seller) %>
                <% end %>
              </td>
              <td class="align-middle">
                <%= link_to item.package.service.title,order_path(item.id) %>
              </td>
              <td class="align-middle"><%= item.created_at.strftime("%d %B %Y") %></td>
              <td class="align-middle"><%= (item.created_at + item.package.delivery_time.days).strftime("%d %B %Y") %></td>
              <td class="align-middle"><%= item.quantity %></td>
              <td class="align-middle">
                <td class="align-middle">
                <div class="btn-group pull-right">
                  <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">Actions
                    <span class="caret"></span>
                  </button>
                  <ul class="dropdown-menu" role="menu">
                    <li class="nav-item">
                      <% if item.seller_star_status.blank? %>
                        <%= link_to "FeedBack",order_feedback_path(item),class: "nav-link" %>
                      <% else %>
                        <%= link_to "View FeedBack",order_feedback_path(item),class: "nav-link" %>
                      <% end %>
                    </li>
                  </ul>
                </div>
              </td>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>