<div class="container-fluid px-5">
    <div class="row">
      <div class="col-sm-12 col-md-12 col-lg-12">
        <div class="SortBlufLink">
          <h2>Earnings</h2>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="card" style="width: 18rem;">
        <div class="card-body text-center">
          <h5 class="card-title">NET Income</h5>
          <h5 class="card-text">$0</h5>
        </div>
      </div>
      <div class="card" style="width: 18rem;">
        <div class="card-body text-center">
          <h5 class="card-title">Withdrawn</h5>
          <h5 class="card-text">$0</h5>
        </div>
      </div>
      <div class="card" style="width: 18rem;">
        <div class="card-body text-center">
          <h5 class="card-title">Pending Clearance</h5>
          <h5 class="card-text">$0</h5>
        </div>
      </div>
      <div class="card" style="width: 18rem;">
        <div class="card-body text-center">
          <h5 class="card-title">Available for Withdraw</h5>
          <h5 class="card-text">$0</h5>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="SortBlufLink">
        <h2>Transaction</h2>
      </div>
      <table class="table responsive table-hover table-condensed mb25" data-sortable data-sortable-initialized="true">
        <thead>
          <tr>
            <th>Date</th>
            <th>For</th>
            <th>Amount</th>
          </tr>
        </thead>
        <tbody>
          <%# @order_active.each do |item| %>
            <tr>
              <td class="align-middle"></td>
              <td class="align-middle"></td>
              <td class="align-middle"></td>
            </tr>
          <%# end %>
        </tbody>
      </table>
    </div>
  </div>
  </div>
</section>
