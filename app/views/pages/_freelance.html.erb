<div class="container pb-3">
  <h3 class="bold text-center">Explore The Marketplace</h3>
  <div class="container">
    <center>
      <ul class="nav nav-tabs" role="tablist">
        <li class="nav-item">
          <a class="nav-link active" href="#cli" data-toggle="tab">
            <%= image_tag "img2.png",width: "30px", height: "30px" %> 
            CLIENTS
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#free" data-toggle="tab">
            <%= image_tag "img3.png",width: "30px", height: "30px" %>FREELANCER
          </a>
        </li>
      </ul>
    </center>  
    <div class="tab-content">
      <%= render 'pages/freelancer/cli' %>
      <%= render 'pages/freelancer/free' %>
    </div>
  </div>
</div>  
<style type="text/css">
  .nav-tabs .nav-item.show .nav-link, .nav-tabs .nav-link.active {
    color: #495057;
    background-color:#95464600;
    border-color:#426f9b #DEE2E6 #fff;
  }
  .nav-tabs .nav-link:hover{
    border-bottom: 3px solid green;
  }
  .nav-tabs {
    border-bottom: 1px solid  #DEE2E6;
    width: 26%;
  }
  a.nav-link{
    width:121.2px;
    height: 74px;
  }
  .images{
    height: 50px;
    width: 50px;
  }
  .categories{
    color: white;
    background-color: rgb(0,0,255, 0.6);
    font-size: 14px;
    padding: 2px 3px 2px 3px;
  }
</style>
<script type="text/javascript">
  $('.nav-tabs a').click(function(){
    $(window).off("scroll", scrollHandler);
  });
</script>

