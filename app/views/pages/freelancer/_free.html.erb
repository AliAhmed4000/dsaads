<div id="free" class="container tab-pane fade"><br>
  <div class="container">
    <!-- <h2 class="display-5 text-center py-3">How Its Work</h2> -->
    <div class="row">
      <% Freelancer.all.each do |f| %>
        <div class="col-md-3">
          <center>
            <%= image_tag f.image_url, class: "images"%>
          </center>
          <h2 class="display-5 text-center"><%= f.heading %></h2>
          <p class="text-center display-6"><%= f.instruction %></p>
        </div>
      <% end %>
    </div>
  </div>
</div>